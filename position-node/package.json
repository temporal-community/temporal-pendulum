{
  "name": "position-node",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "start": "ts-node src/worker.ts",
    "start.watch": "nodemon src/worker.ts",
    "test": "mocha lib/*.test.js",
    "test.coverage": "rm -rf ./coverage && npm run build && nyc instrument lib lib --in-place && env COVERAGE=1 mocha lib/*.test.js && nyc report -t coverage --reporter=lcov --reporter=text-summary"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "temporalio": "^1.0.0-rc.0"
  },
  "devDependencies": {
    "@temporalio/testing": "^1.0.0-rc.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^28.1.3",
    "@types/mocha": "9.x",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "jest": "^28.1.1",
    "mocha": "9.x",
    "nodemon": "^2.0.14",
    "nyc": "15.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-mocha": "9.x",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2"
  }
}
